{
  "name": "rclnodejs",
  "version": "0.0.2",
  "description": "ROS2.0 JavaScript client with Node.js",
  "main": "index.js",
  "keywords": [
    "rclnodejs",
    "ros2",
    "ros",
    "rcl"
  ],
  "scripts": {
    "install": "node-gyp rebuild",
    "docs": "cd docs && make",
    "test": "node ./scripts/compile_cpp.js && node ./scripts/run_test.js",
    "lint": "eslint --max-warnings=0 index.js scripts lib example rosidl_gen rosidl_parser test && node ./scripts/cpplint.js"
  },
  "authors": [
    "Minggang Wang <minggang.wang@intel.com>",
    "Kenny Yuan <kaining.yuan@intel.com>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotWebTools/rclnodejs.git"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "mocha": "^3.5.0"
  },
  "dependencies": {
    "bindings": "^1.2.1",
    "debug": "^2.6.8",
    "dot": "^1.1.2",
    "fs-extra": "^3.0.1",
    "mz": "^2.6.0",
    "mkdirp": "^0.5.1",
    "ref": "1.3.4",
    "ref-struct": "^1.1.0",
    "ref-array": "^1.2.0",
    "walk": "^2.3.9"
  }
